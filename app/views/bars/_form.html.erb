<%= form_for(@bar, :html => {:multipart => true}) do |f| %>
  <%= f.label :name%>
  <%= f.text_field :name %>
  <br>
  <%= f.label :image %>
  <%= f.cl_image_upload :image%>
  <br>
  <%= f.label :description%>
  <%= f.text_field :description %>
  <br>
  <%= f.label :phone%>
  <%= f.text_field :phone %>
  <br>
  <%= f.label :website%>
  <%= f.text_field :website %>
  <br>
  <%= f.submit %>
<% end %>
